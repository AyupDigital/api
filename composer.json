{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "GNU AGPLv3",
    "type": "project",
    "require": {
        "php": "^7.4.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "alphagov/notifications-php-client": "^2",
        "babenkoivan/scout-elasticsearch-driver": "^4.2.3",
        "bugsnag/bugsnag-laravel": "^2.20",
        "doctrine/dbal": "^2.13",
        "erusev/parsedown": "^1.7",
        "firebase/php-jwt": "~3.0",
        "goldspecdigital/oooas": "^2.8",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.83",
        "laravel/passport": "^10.0",
        "laravel/tinker": "^2.5",
        "lcobucci/jwt": "3.3.3",
        "league/flysystem-aws-s3-v3": "~1.0",
        "mailgun/mailgun-php": "^3.0",
        "nyholm/psr7": "^1.2",
        "php-http/guzzle6-adapter": "^2.0",
        "phpoffice/phpspreadsheet": "^1.4",
        "predis/predis": "^1.1",
        "spatie/laravel-query-builder": "^4.0",
        "superbalist/laravel-google-cloud-storage": "^2.2.2",
        "symfony/yaml": "^5.4",
        "twilio/sdk": "^6.6",
        "fruitcake/laravel-cors": "^2.0",
        "laravel/ui": "^3.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.2",
        "fakerphp/faker": "^1.16",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "pda/pheanstalk": "~4.0",
        "phpunit/phpunit": "^9.5.10",
        "facade/ignition": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "bootstrap/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "test": [
            "@test:style",
            "@test:unit"
        ],
        "test:style": [
            "@fix:style --dry-run"
        ],
        "test:unit": [
            "@php vendor/bin/phpunit"
        ],
        "fix:style": [
            "@php vendor/bin/php-cs-fixer fix --config=.php_cs_fixer.php --allow-risky=yes --verbose"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 0,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
